{
  "type": "module",
  "name": "polka-jwt",
  "version": "0.0.1",
  "description": "Polka middleware for validating JWT's, heavily inspired from express-jwt",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "cleanup": "shx rm -rf dist",
    "prebuild": "npm run cleanup && npm run verify",
    "build": "rollup -c rollup.config.js --environment NODE_ENV:production",
    "test": "NODE_ENV=test jest -c ./jest.config.js --detectOpenHandles",
    "test:debug": "NODE_ENV=test jest --env=node",
    "test:watch": "npm t -- --watch",
    "test:coverage": "npm t -- --coverage",
    "test:ci": "npm t -- --ci",
    "validate-js": "tsc -p ./tsconfig.json",
    "verify": "npm run style && npm run test:ci",
    "style": "npm run format -- --list-different && npm run lint",
    "style:fix": "npm run format:fix && npm run lint:fix",
    "format": "prettier \"**/*.{ts,js}\"",
    "format:fix": "npm run format -- --write",
    "lint": "tslint --project tsconfig.json --format codeFrame",
    "lint:fix": "npm run lint -- --fix",
    "pre-publish": "npm shrinkwrap"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eve-id/polka-jwt.git"
  },
  "homepage": "https://github.com/eve-id/polka-jwt.git",
  "keywords": [],
  "author": "Anastasios Soukoulis",
  "license": "MIT",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "7.11.0",
    "@babel/plugin-transform-typescript": "7.11.0",
    "@babel/preset-env": "7.11.5",
    "rollup": "2.26.11",
    "rollup-plugin-esnext": "0.1.0",
    "rollup-plugin-sourcemaps": "0.6.2",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.27.2",
    "@rollup/plugin-babel": "5.2.1",
    "@rollup/plugin-commonjs": "15.0.0",
    "@rollup/plugin-node-resolve": "9.0.0",
    "@types/express": "4.17.8",
    "@types/jest": "26.0.13",
    "@types/jsonwebtoken": "8.5.0",
    "@types/just-safe-set": "2.1.0",
    "@types/node": "14.10.0",
    "@types/polka": "0.5.1",
    "fast-jwt": "git+https://github.com/eve-id/fast-jwt.git#master",
    "jest": "26.4.2",
    "just-safe-set": "2.1.0",
    "prettier": "2.1.1",
    "shx": "0.3.2",
    "ts-jest": "26.3.0",
    "ts-node": "9.0.0",
    "tslib": "2.0.1",
    "tslint": "6.1.3",
    "typescript": "4.0.2",
    "typescript-tslint-plugin": "0.5.5"
  }
}
